<h1>All equipment</h1>
<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <table class="table table-hover" id="equipment-table">
            <thead>
                <tr>
                    <th>#</th>
                    <th>Model</th>
                    <th>Type</th>
                    <th>Location</th>
                    <th>Status</th>
                    <th>Warranty Status</th>
                </tr>
            </thead>
            <tbody>
                <%= render @equipment || "No equipment found" %>
            </tbody>
        </table>
    </div>
</div>