
<h2>New equipment requests</h2>
<div class="row">
    <div class="col-md-offset-3 col-md-6">
        <table class="table table-hover" id="equipment-table">
            <thead>
                <tr>
                    <th>#</th>
                    <th>Model</th>
                    <th>Type</th>
                    <th>Lab Owner ID</th>
                    <th>Status</th>
                </tr>
            </thead>
            <tbody>
                <%= render @requests %>
            </tbody>
        </table>
    </div>
</div>

<hr>

<h2>Move requests</h2>
<div class="row">
    <div class="col-md-offset-3 col-md-6">
        <table class="table table-hover" id="move-requests-table">
            <thead>
                <tr>
                    <th>Equipment #</th>
                    <th>From Lab Owner</th>
                    <th>To Lab Owner</th>
                    <th>HOD Approval Status</th>
                    <th>Receiver Lab Owner Approval Status</th>
                </tr>
            </thead>
            <tbody>
                <%= render @move_requests %>
            </tbody>
        </table>
    </div>
</div>