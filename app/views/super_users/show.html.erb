
<h1>All requests</h1>
<div class="row">
    <div class="col-md-offset-3 col-md-6">
        <%= will_paginate @requests %>
        <table class="table table-hover">
            <thead>
                <tr>
                    <th>#</th>
                    <th>Model</th>
                    <th>Type</th>
                    <th>Status</th>
                </tr>
            </thead>
            <tbody>
                <%= render @requests %>
            </tbody>
        <%= will_paginate @requests %>
    </div>
</div>